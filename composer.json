{
    "name": "vairogs/vairogs-search",
    "description": "Utils: ElasticSearch library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "search",
        "elasticsearch",
        "elastic",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "vairogs/vairogs-core": "dev-master",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/http-foundation": "*",
        "doctrine/annotations": "~1.6",
        "doctrine/collections": "~1.5",
        "symfony/framework-bundle": "*",
        "symfony/console": "*",
        "psr/log": "^1",
        "symfony/event-dispatcher": "*",
        "elasticsearch/elasticsearch": "^6.0",
        "doctrine/cache": "~1.7",
        "monolog/monolog": "^1",
        "symfony/var-dumper": "*",
        "symfony/options-resolver": "*",
        "symfony/stopwatch": "*",
        "symfony/templating": "*",
        "symfony/serializer": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Utils\\Search\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Utils\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
